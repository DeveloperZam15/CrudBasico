@model IEnumerable<CrudBasico.Models.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UsuMatricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuNombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuApellidos)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuClave)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UsuMatricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsuNombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsuApellidos)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UsuClave)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UsuMatricula}, new {@class="btn btn-warning"}) |
            @Html.ActionLink("Details", "Details", new { id=item.UsuMatricula}, new {@class="btn btn-primary"}) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UsuMatricula}, new {@class="btn btn-danger"})
        </td>
    </tr>
}

</table>
